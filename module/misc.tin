#alias {zbg.recover.do} {
    #alias cb_zbg #cr;
    #alias cb_zbg %0;
    
    #if {$status[hp_max] > $status[hp_valid} {
        yun heal
        #delay {1} {zbg.recover.do};
    };
    #elseif {$status[hp_valid] > $status[hp]} {
        yun recover
        #delay {1} {zbg.recover.do};
    };
    #elseif {$status[sp_max] > $status[sp_valid} {
        yun inspire;
        #delay {1} {zbg.recover.do};
    };
    #elseif {$status[sp_valid] > $status[sp]} {
        yun regenerate;
        #delay {1} {zbg.recover.do};
    };
    #else {
        cb_zbg;
    };
};
