#alias {climbtree} {
    #if {"%1" == "stop"} {
        status.set_action 空闲;
        #class climb_tree_inner kill;
    } {
        status.set_action 爬树;
        #class climb_tree_inner open;
        
        #act {^你领悟出一些基本轻功方面的窍门} {
            #delay {1.5} {climb tree};
        };
        #act {^你减缓真气运行，让气血运行恢复正常} {
            yun qi;
        };
        #act {^你现在正忙着呢} {
            #delay {1} {he jiudai;eat liang;climb tree};
        };
        #act {^你身子发虚，一头栽了下来，哎呀} {
