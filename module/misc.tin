#alias {baij} {
    #If {"%1" == "stop"} {
        status.set_action 空闲;
        #class baij_inner kill;
    } {
        status.set_action 拜祭;
        #class baij_inner open;
        
        #var baiji_count 10;
        
        #act {^你减缓真气运行，让自身恢复正常} {
            yun jing;
        };
        #act {^你的精不够，无法继续拜祭} {
            baij stop;
            
