#alias {zyqsq} {
    #if {"%1" == "stop"} {
        status.set_action 空闲;
        #class zyqsq.inner kill;
    } {
        status.set_action 钻研七伤;
        
        #class zyqsq.inner open;

        #var count 10;
        #var zy_mode 1;
        
        #act {^你正读着七伤拳谱，突然感觉经脉运行不顺} {
            zyqsq stop;
            #nop #delay {qsq}{zyqsq}{60};
        };
        #act {^你减缓真气运行，让自身恢复正常} {
            #if {$status[force] > @eval{$status[force_max] * 0.95}} {
                log 内力充足 全速钻研;
                #var zy_mode 1;
                #var count 10;
            } {
                yun jing;
            };
        };
        #act {^你已经很累了，歇歇再读吧,身体要紧} {
            #if {$zy_mode == 1} {
                log yun regenerate;
                yun regenerate;
            } {
                log 钻研累了 放慢速度;
                yun jing;
                #if {$count != 1} {
                    #var count 1;
                };
            };
        };
        
        #tic {tk_zuanyan} {
            #if {$zy_mode == 1} {
                #if {$status[force] < @eval{$status[force_max] * 0.5}} {
                    log 内力不够 放慢速度;
                    #var zy_mode 0;
                    yun jing;
                };
            } {
                #if {$status[sp] < @eval{$status[sp_max] * 0.5}} {
                    #if {$count != 1} {
                        log 精神欠佳 慢慢钻研;
                        hpbrief1;
                        #var count 1;
                    };
                };
                #elseif {$status[sp] > @eval{$status[sp_max] * 0.95}} {
                    #if {$count != 10} {
                        log 精神充沛 快速钻研;
                        #var count 10;
                        hpbrief1;
                    };
                };
            };
            zuanyan book $count;
        } {1};
        
        #tic {tk_chihe} {eat liang;he jiudai} {100};
            
        #class zyqsq.inner open;
    }; 
};

#alias {filljiudai} {
    
    #if {$fjd[count] > 0} {
        fill jiudai;
        drop jiudai;
        
        #math fjd[count] {$fjd[count] - 1};
        
        #delay {0.5} {filljiudai};
    } {
        #show 酒袋装填完毕;
        get all;
    };
};
#alias {fjd} {
    #var fjd[count] %1;
    
    filljiudai;
    
};

#alias {ch} {
    #if {"%1" == "stop"} {
        #class ch.inner kill;
    } {
        #class ch.inner open;
        
        #act {^你感觉心中的杀气渐渐消失} {
            chanhui;
        };
        #act {^{你心中已没有丝毫杀气|你心中一片平和，已经没有丝毫杀气}} {
            ch stop;
        };
        
        #class ch.inner close;
        
        chanhui;
    };
};

#alias {maijd} {
    
    #class mai.inner open;
    
    #var item[need] %2;
    #var item[own] 0;
    #var item[id] %1;
    
    #act {^你向当铺买下一个牛皮酒袋} {
        #math item[own] {$item[own] + 1};
        
        #if {$item[own] == $item[need]} {
            #show $item[need]个 $item[id] 购买完毕;
            #class mai.inner kill;
        } {
            #delay {1} { buy $item[id]};
        };
    };
    #act {^你现在正忙着呢} {
        #delay {1} { buy $item[id]};
    };
    
    #class mai.inner close;
    buy $item[id];
};

