#alias {dushu.start} {
    #if {"%1" != ""} {
        #var dushu[count] %1;
    };
    
    #class dushu.start.inner open;
    
    #tic {dushu} {
        #if {$status[sp] < @eval{$status[sp_max] * 0.2}} {
            hpbrief1;
