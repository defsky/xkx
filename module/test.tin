};

#alias {tm.start} {
    
    #class tm.dazuo.inner kill;
    
    #if {"%1" != "" || &tm[target] != 0} {
        #var tm[target] %1;
        
        log 开始通脉: $tm[target];
        
        #class tm.start.inner open;
        
        #act {^你的真气在%*上%*穴运行受阻，还是尽快想办法把受损的经脉恢复吧} {
            log 经脉受损: %%1 %%2;
            #var tm[fix][jing] {%%1};
            #var tm[fix][xue] {%%2};
            
            tm.fix;
        };
        #act {^你已将内力纳入$tm[target]穴} {
            #undelay {tm_checker};
            vein through $tm[target];
        };
        #act {^你现在内力不足，强行通脉，有害无益} {
            tm.stop;
            eat liang;
            he jiudai;
            tm.dazuo;
        };
        #act {^你此处冲穴消耗了%*点内力} {
            #delay {tm_checker} {
                tm.start;
            }{30};
        };
        #act {^现在你只能尝试往%*里灌注内力}{
            log 贯通下个穴道：%%1;
            #var tm[target] %%1;
            tm.stop;
            tm.start;
        };
        #act {^过多尝试通脉次数，有害无益。今天就到此为止吧} {
            tm.stop;
            log 当天通脉次数用尽;
            #show MISSION_COMPLETE;
        };
        
        #class tm.start.inner close;
        
        vein through $tm[target];
    } {
        #show <139>请提供穴道名<079>;
    };
};

