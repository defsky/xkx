#alias {slw} {
    #if {"%2" == ""} {
        #show <139>slw <name> <step><079>;
        #return;
    };
    
    #var slw[target] %1;
    #var slw[dist] %2;
    #var slw[walked] 0;
    
    #act {^%* -  $} {
        #math slw[walked] {$slw[walked] + 1};
        #if {$slw[walked] >= $slw[dist]} {
            #delay {1} {#show <139>walk step : $slw[walked]<079>;};
            walk -p;
            wtmsg;
            #unact {^%* -  $};
        } {
            #delay {1} {#show <139>walk step : $slw[walked]<079>;};
        };
    };
    walk $slw[target];
};

